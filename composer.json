{
  "name": "tjmugova/bluedot-sms",
  "description": "Provides BluedotSms notification channel for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Tererai Mugova",
      "email": "tjmugova@webstudio.co.zw"
    }
  ],
  "keywords": [
    "laravel-sms",
    "notifications",
    "bluedot"
  ],
  "require": {
    "php": "^8.2",
    "illuminate/notifications": "^11.0 || ^12.0",
    "illuminate/support": "^11.0 || ^12.0",
    "illuminate/events": "^11.0 || ^12.0",
    "illuminate/queue": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Tjmugova\\BluedotSms\\": "src",
      "Tjmugova\\BluedotSms\\Tests\\": "tests"
    },
    "files": []
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tjmugova\\BluedotSms\\BluedotSmsServiceProvider"
      ],
      "aliases": {
        "BluedotSms": "Tjmugova\\BluedotSms\\BluedotSms"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  }
}
