{
    "name": "tjmugova/bluedot-sms",
    "description": "Provides BluedotSms notification channel for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tererai Mugova",
            "email": "tjmugova@webstudio.co.zw"
        }
    ],
    "keywords": [
        "laravel-sms",
        "notifications",
        "bluedot"
    ],
    "require": {
        "php": ">=7.2|^8.0",
        "illuminate/notifications": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/events": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/queue": "^5.8 || ^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tjmugova\\BluedotSms\\": "src",
            "Tjmugova\\BluedotSms\\Tests\\": "tests"
        },
        "files": []
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjmugova\\BluedotSms\\BluedotSmsServiceProvider"
            ],
            "aliases": {
                "BluedotSms": "Tjmugova\\BluedotSms\\BluedotSms"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
